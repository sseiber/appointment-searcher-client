{
    "name": "appointment-searcher-client",
    "version": "3.0.4",
    "description": "LoopBox local client",
    "main": "index.ts",
    "config": {
        "imageName": "sseiber.azurecr.io/appointment-searcher-client"
    },
    "scripts": {
        "test": "node ./node_modules/jest/bin/jest.js",
        "build": "./node_modules/.bin/webpack --color --config configs/webpack.prod.js --mode production",
        "preversion": "npm test",
        "version": "npm run build",
        "postversion": "cp -r client_dist ../Loopbox-Local-Service",
        "start": "./node_modules/.bin/webpack serve --config ./configs/webpack.dev.js --mode development"
    },
    "author": "sseiber",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:sseiber/Appointment-Searcher-Client.git"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "^17.0.1",
        "@types/history": "^4.7.8",
        "@types/jest": "^26.0.19",
        "@types/node": "^14.14.16",
        "@types/react": "^17.0.0",
        "@types/react-test-renderer": "^17.0.0",
        "@types/webpack-env": "^1.16.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "commander": "^6.2.1",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.0.0-alpha.17",
        "jest": "^26.6.3",
        "mini-css-extract-plugin": "^1.3.3",
        "node-sass": "^5.0.0",
        "react-hot-loader": "^4.13.0",
        "react-test-renderer": "^17.0.1",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.11.1",
        "webpack-cli": "^4.3.0",
        "webpack-dev-server": "^3.11.1"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "eventemitter2": "^4.1.0",
        "fs-extra": "^9.0.1",
        "history": "^5.0.0",
        "loopbox-types": "^2.0.5",
        "mobx": "^6.0.4",
        "mobx-react": "^7.0.5",
        "query-string": "^6.13.7",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "semantic-ui-react": "^2.0.1",
        "whatwg-fetch": "^3.5.0"
    },
    "jest": {
        "rootDir": "./src/",
        "testRegex": "(/__tests__/.*|\\.spec)\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "ts",
            "tsx"
        ],
        "transform": {
            ".tsx?": "ts-jest"
        },
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js"
        }
    }
}
